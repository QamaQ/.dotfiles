# NORD THEME
set default-bg              "#212121" # nord0
set default-fg              "#EEFFFF" # nord4
set statusbar-bg            "#212121" # nord0
set statusbar-fg            "#EEFFFF" # nord4
set index-bg                "#212121" # nord0
set index-fg                "#EEFFFF" # nord4
set index-active-bg         "#212121" # nord2
set index-active-fg         "#EEFFFF" # nord4
set recolor-darkcolor       "#EEFFFF" # nord4
set recolor-lightcolor      "#212121" # nord0
set render-loading-bg       "#212121" # nord0
set render-loading-fg       "#EEFFFF" # nord4set default-bg "#000000" #00
set default-fg "#EEFFFF" #01

set font "JetBrainsMono Nerd Font Bold 9"

map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map i recolor
map p print
map F toggle_fullscreen
map [fullscreen] F toggle_fullscreen
